    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.1.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.3.3",
    "cypress-visual-regression": "^5.3.0",
    "daisyui": "^5.0.35",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.2.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.17",
    "typescript-eslint": "^8.32.1",
    "vite": "^5.4.19",
    "vitest": "^1.6.1",
    "wait-on": "^8.0.3"
  }
